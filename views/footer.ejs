<script>
    $(function(){
        var height = $("body").outerHeight();
        var view_height = $(window).outerHeight();
        if(parseInt(height + 200) < view_height){
            $("body").height(view_height);
            $("#footer").css({
                "position":"absolute",
                "bottom":"0px",
                "left":"0px",
                "width":"100%",
            });
        }
    })
</script>